{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2017",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": ["examples/**/*.ts", "examples/**/*.tsx"]
}
